# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

# CocoaPods analytics sends network stats synchronously affecting flutter build latency.
ENV['COCOAPODS_DISABLE_STATS'] = 'true'

project_id = 'Runner'
project_path = File.expand_path("..", __FILE__)
ios_project_path = File.join(project_path, "ios")

# Prevent Cocoapods from adding a script phase to the project that runs `pod install` automatically.
# This script phase is not needed for Flutter projects and can cause issues with Xcode.
install! 'cocoapods', :disable_input_output_paths => true

# Keep pod install from freezing on M1 Macs
if `uname -m` == "arm64"
  ENV['COCOAPODS_DISABLE_STATS'] = 'true'
end

# Define the Flutter installation directory
flutter_application_path = File.join(project_path, '.ios', 'Flutter')

# Define the Flutter framework directory
flutter_framework_dir = File.join(flutter_application_path, 'engine')

# Define the Flutter build directory
flutter_build_dir = File.join(flutter_application_path, 'build')

# Define the Flutter build mode
